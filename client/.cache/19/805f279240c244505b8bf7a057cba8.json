mn":0},"generated":{"line":108,"column":0}},{"source":"src/components/articleListItem.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":109,"column":0}},{"source":"src/components/articleListItem.vue","name":null,"original":{"line":44,"column":0},"generated":{"line":110,"column":0}}],"sources":{"src/components/articleListItem.vue":"<template>\n  <div class=\"row  mb-4 pt-5 px-5\" style=\"border:10px solid #343a40;\">\n    <div class=\"col-6 mb-5\">\n      <h4 class=\"card-title\">{{article.title}}</h4>\n      <div v-line-clamp:20=\"2\">\n\n      <p class=\"card-text\" style=\"overflow:hidden;\" v-html=\"article.content\"></p>\n      </div>\n      <p style=\"font-size:14px;\">Author : {{article.UserId.username}}</p>\n      <p class=\"card-text\">\n        <small\n          class=\"text-muted\"\n        >{{new Date(article.createdAt).toLocaleDateString(\"en-US\", { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' })}}</small>\n      </p>\n      <div style=\"display:flex;justify-content:space-around\">\n      <p>tags : </p>\n      \n        <a href=\"#\" @click=\"searchByTag(tag)\"   v-for=\"(tag,i) in article.tags\" :key=\"i\">\n        <vs-chip>{{tag}}</vs-chip>\n        </a>\n      \n      </div>\n     \n      <button v-on:click=\"detailedArticle(article)\" class=\"btn btn-secondary btn-sm\">details</button>\n      <button\n        v-if=\"isUserArticle\"\n        v-on:click=\"editArticle(article)\"\n        class=\"btn btn-secondary btn-sm\"\n      >edit</button>\n      <button\n        v-if=\"isUserArticle\"\n        v-on:click=\"removeArticle(article._id)\"\n        class=\"btn btn-secondary btn-sm\"\n      >delete</button>\n    </div>\n    <div class=\"col-6\">\n      <img class=\"img-fluid mb-5\" :src=\"article.featured_image\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  props: {\n    article: Object,\n    isUserArticle: Boolean\n  },\n  data() {\n    return {};\n  },\n  methods: {\n    removeArticle(id) {\n      let token = localStorage.getItem(\"token\");\n      console.log(id);\n\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        type: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\"\n      }).then(result => {\n        if (result.value) {\n          return axios({\n            url: `http://localhost:3000/articles/${id}`,\n            method: \"DELETE\",\n            headers: {\n              token\n            }\n          })\n            .then(response => {\n              console.log(response);\n              Swal.fire(\"Deleted!\", \"Your file has been deleted.\", \"success\");\n              this.$emit(\"removedArticle\", id);\n            })\n            .catch(err => {\n              let message = err.response && err.response.data.message || \"failed to delete data\"\n              Swal.fire({\n                type : \"error\",\n                title : \"failed to delete article\",\n                text : message\n              })\n              // console.log(err);\n            });\n        }\n      });\n    },\n    editArticle(obj) {\n      this.$emit(\"editArticle\", obj);\n    },\n    detailedArticle(obj) {\n      console.log(\"list item detail\");\n      this.$emit(\"detailedArticle\", obj);\n    },\n    searchByTag(tag){\n      this.$emit(\"searchByTag\", tag)\n\n    }\n\n  }\n};\n</script>\n\n<style scoped>\n</style>\n"},"lineCount":null}},"error":null,"hash":"714b362600a98eecac4554afbf5410fb","cacheData":{"env":{}}}