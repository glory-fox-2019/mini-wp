{"line":92,"column":36}},{"source":"src/components/rightcolumn.vue","name":null,"original":{"line":86,"column":0},"generated":{"line":93,"column":0}},{"source":"src/components/rightcolumn.vue","name":null,"original":{"line":39,"column":0},"generated":{"line":94,"column":0}},{"source":"src/components/rightcolumn.vue","name":null,"original":{"line":32,"column":0},"generated":{"line":95,"column":0}}],"sources":{"src/components/rightcolumn.vue":" <template>\n  <div class=\"isi\">\n    <div class=\"bottom\" v-for=\"(article,index) in articles\" :key=\"index\">\n      <div class=\"judul\">\n        <div>\n          <h4>{{article.title}}</h4>\n          <p v-html=\"article.content\"></p>\n          <p>by {{article.author}}</p>\n          <p>{{String(new Date(article.createdAt)).substr(0,10)}}</p>\n          <div>\n            <p>tags:</p>\n            <a href=\"#\" @click=\"getTags(t)\" :key=\"i\" v-for=\"(t,i) in article.tags\" ><vs-chip >{{t}}</vs-chip></a>\n          </div>\n        </div>\n        <div class=\"photo\">\n          <div class=\"ph\">\n            <img :src=\"article.featured_image\" alt=\"foto\" />\n          </div>\n          <div class=\"more\">\n            <div class=\"dots\">\n              <a class=\"publish\" href=\"#\" @click=\"deleteItem(article._id)\">delete</a>\n              <a class=\"publish\" href=\"#\" @click=\"clickEdit(article)\">edit</a>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"articles\", \"tempArticles\"],\n  data() {\n    return {\n      filter: \"\"\n    };\n  },\n  methods: {\n    getTags(name) {\n      this.$emit('tagss', name)\n    },\n    deleteItem(input) {\n      let id = input;\n      let token = localStorage.getItem(\"token\");\n      Swal.fire({\n        title: \"Are you sure?\",\n        text: \"You won't be able to revert this!\",\n        type: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Yes, delete it!\"\n      })\n        .then(result => {\n          if (result.value) {\n            axios({\n              method: \"DELETE\",\n              url: `http://localhost:3000/articles/delete/${id}`,\n              headers: {\n                token\n              }\n            }).then(data => {\n              let response = data.data.data;\n              if (response) {\n                this.$emit(\"delete-article\", id);\n                Swal.fire(\"Deleted!\", \"Your file has been deleted.\", \"success\");\n              } else {\n                Swal.fire({\n                  position: \"center\",\n                  type: \"error\",\n                  title: \"Forbidden\",\n                  showConfirmButton: false,\n                  timer: 1500\n                });\n              }\n            });\n          }\n        })\n        .catch(err => {\n          console.log(err);\n        });\n    },\n    clickEdit(input) {\n      this.$emit(\"updateFile\", input);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.published {\n  margin-right: 15%;\n}\n.published:hover {\n  border-bottom: 1px solid rgba(33, 117, 155, 0.5);\n}\n\n.publish {\n  color: #21759b;\n}\n#draft {\n  color: #21759b;\n}\n\n.isi {\n  height: 50vh;\n}\n.bottom {\n  margin-bottom: 5%;\n  padding: 5%;\n  background-color: white;\n  -webkit-box-shadow: 3px 6px 5px 0px rgba(0, 0, 0, 0.75);\n  -moz-box-shadow: 3px 6px 5px 0px rgba(0, 0, 0, 0.75);\n  box-shadow: 3px 6px 5px 0px rgba(0, 0, 0, 0.75);\n  height: 220px;\n}\n.judul {\n  display: flex;\n  justify-content: space-between;\n}\n.photo {\n  display: flex;\n}\n.ph {\n  margin-right: 10%;\n}\na {\n  text-decoration: none;\n}\nimg {\n  width: 100px;\n  height: 70px;\n}\n.clickaa {\n  cursor: grab;\n}\n</style>\n            \n            \n"},"lineCount":null}},"error":null,"hash":"37adabb43b408beca429b4efe642bade","cacheData":{"env":{}}}